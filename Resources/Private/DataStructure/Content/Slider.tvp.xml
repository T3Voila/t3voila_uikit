<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <meta>
        <title>Multicontent Slider</title>
        <description>Place a description here</description>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <el>
                    <field_parallax>
                        <label>Parallax</label>
                        <config>
                            <type>check</type>
                            <items>
                                <numIndex index="0">
                                    <label>Use parallax</label>
                                    <value>1</value>
                                </numIndex>
                            </items>
                        </config>
                    </field_parallax>
                    <field_center>
                        <label>Center</label>
                        <config>
                            <type>check</type>
                            <items>
                                <numIndex index="0">
                                    <label>Centered active slide</label>
                                    <value>1</value>
                                </numIndex>
                            </items>
                        </config>
                    </field_center>
                    <field_space>
                        <label>Space between</label>
                        <config>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <items>
                                <numIndex index="0">
                                    <label>Small</label>
                                    <value>small</value>
                                </numIndex>
                                <numIndex index="1">
                                    <label>Medium</label>
                                    <value>medium</value>
                                </numIndex>
                                <numIndex index="2">
                                    <label>Large</label>
                                    <value>large</value>
                                </numIndex>
                                <numIndex index="3">
                                    <label>None</label>
                                    <value>none</value>
                                </numIndex>
                            </items>
                        </config>
                    </field_space>
                    <field_width>
                        <label>Width per element</label>
                        <config>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <items>
                                <numIndex index="0">
                                    <label>100%</label>
                                    <value>1-1</value>
                                </numIndex>
                                <numIndex index="1">
                                    <label>83,3%</label>
                                    <value>5-6</value>
                                </numIndex>
                                <numIndex index="2">
                                    <label>80%</label>
                                    <value>4-5</value>
                                </numIndex>
                                <numIndex index="3">
                                    <label>75%</label>
                                    <value>3-4</value>
                                </numIndex>
                                <numIndex index="4">
                                    <label>66%</label>
                                    <value>2-3</value>
                                </numIndex>
                                <numIndex index="5">
                                    <label>60%</label>
                                    <value>3-5</value>
                                </numIndex>
                                <numIndex index="6">
                                    <label>50%</label>
                                    <value>1-2</value>
                                </numIndex>
                                <numIndex index="7">
                                    <label>40%</label>
                                    <value>2-5</value>
                                </numIndex>
                                <numIndex index="8">
                                    <label>33%</label>
                                    <value>1-3</value>
                                </numIndex>
                                <numIndex index="9">
                                    <label>25%</label>
                                    <value>1-4</value>
                                </numIndex>
                                <numIndex index="10">
                                    <label>20%</label>
                                    <value>1-5</value>
                                </numIndex>
                                <numIndex index="11">
                                    <label>16,6%</label>
                                    <value>1-6</value>
                                </numIndex>
                            </items>
                        </config>
                    </field_width>
                    <field_sections>
                        <type>array</type>
                        <el>
                            <field_section>
                                <type>array</type>
                                <el>
                                    <field_name>
                                        <label>Section Name</label>
                                        <config>
                                            <type>input</type>
                                            <size>48</size>
                                            <eval>trim</eval>
                                        </config>
                                    </field_name>
                                    <field_content>
                                        <label>Contentelements</label>
                                        <config>
                                            <type>group</type>
                                            <allowed>tt_content</allowed>
                                            <size>5</size>
                                            <maxitems>200</maxitems>
                                            <minitems>0</minitems>
                                            <multiple>1</multiple>
                                        </config>
                                    </field_content>
                                </el>
                                <title>One Section</title>
                            </field_section>
                        </el>
                        <section>1</section>
                        <title>Multiple Sections</title>
                    </field_sections>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
